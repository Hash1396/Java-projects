<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>ServletJDBC</display-name>
  <welcome-file-list>
    <welcome-file>mypage.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>login</servlet-name>
  <servlet-class>Servletjdbc.LoginServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>userpg</servlet-name>
  <servlet-class>Servletjdbc.UserPage</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>userpg</servlet-name>
  <url-pattern>/userpage</url-pattern>
  </servlet-mapping>
  
   <servlet>
  <servlet-name>register</servlet-name>
  <servlet-class>Servletjdbc.Registration</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>register</servlet-name>
  <url-pattern>/addnew</url-pattern>
  </servlet-mapping>
  
   <servlet>
  <servlet-name>editinfo</servlet-name>
  <servlet-class>Servletjdbc.UpdatePage</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>editinfo</servlet-name>
  <url-pattern>/edit</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>updateexe</servlet-name>
  <servlet-class>Servletjdbc.UpdateExecution</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>updateexe</servlet-name>
  <url-pattern>/update</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>lgout</servlet-name>
  <servlet-class>Servletjdbc.LogoutServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>lgout</servlet-name>
  <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>display</servlet-name>
  <servlet-class>Servletjdbc.DisplayServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>display</servlet-name>
  <url-pattern>/disp</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>SignUP</servlet-name>
  <servlet-class>Servletjdbc.SignUpPage</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>SignUP</servlet-name>
  <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>Delete</servlet-name>
  <servlet-class>Servletjdbc.DeleteServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>Delete</servlet-name>
  <url-pattern>/delete</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>DelAcc</servlet-name>
  <servlet-class>Servletjdbc.DelAccount</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>DelAcc</servlet-name>
  <url-pattern>/delacc</url-pattern>
  </servlet-mapping>
  
</web-app>